---
import Logo from './Logo.astro'
import Themetoggle from './Themetoggle.astro';
---

<header>
  <div class="fb items-center flex-row mt-6rem">
    <div class="fi">
      <a href="." onclick="return false" mr-1 select-none transition-all flex gap-2>
        <Logo />
        <span class="gpt-subtitle dark:filter-sepia ml-1">Free</span>
        <span class="gpt-title dark:gpt-subtitle dark:filter-invert">Chat</span>
      </a>
    </div>
    <div class="flex">
      <Themetoggle />
    </div>
  </div>
  <iframe src="" class="h-20 w-full bg-$c-fg-2 hover:bg-$c-fg-5 hover:ring-1.75 ring-$c-fg-40 ring-inset transition-all mt-2 mb-1 rounded-lg">测试</iframe>
</header>
